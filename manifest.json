{
  "name": "FREE mapgenie PRO by MrFusiion.",
  "description": "Make unlimited markers and category tracking + extra custom features.",
  "version": "3.0.0",
  "manifest_version": 3,
  "content_scripts": [ 
    {
      "js": [ "inject.js" ],
      "matches": ["*://*/*"],
      "run_at": "document_end"
    }
],
  "web_accessible_resources": [{
    "matches": ["*://*/*"],
    "resources": [ "storage.js", "main.js", "list.js",
                    "map.js", "guide.js", "templates.js"]
  }],
  "icons": {
    "16": "/images/mapgenie_pro_16.png",
    "32": "/images/mapgenie_pro_32.png",
    "64": "/images/mapgenie_pro_64.png",
    "128": "/images/mapgenie_pro_128.png"
  }
}